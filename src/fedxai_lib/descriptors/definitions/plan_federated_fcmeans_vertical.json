{
   "name": "Federated Fuzzy c-means algorithm for vertically data partitioning",
   "server_class": "fedxai_lib.algorithms.federated_fcmeans_vertical.server.FederatedVerticalFCMServer",
   "client_class": "fedxai_lib.algorithms.federated_fcmeans_vertical.client.FederatedVerticalFCMClient",
   "stages": [
       {
          "name": "Initialization of parties",
          "type": "run_once",
          "pipeline": [
             {
                "entity": "#server",
                "method": "init"
             },
             {
                "entity": "#available_clients",
                "method": "init"
             }
          ]
       },
       {
          "name": "Run federation",
          "type": "run_many",
          "stop_condition": "stop_federation",
          "pipeline": [
             {
                "entity": "#server",
                "method": "get_cluster_stats"
             },
             {
                "entity": "#available_clients",
                "method": "update_local_centers"
             },
             {
                "entity": "#server",
                "method": "process_round"
             }
          ]
       }
   ]
}