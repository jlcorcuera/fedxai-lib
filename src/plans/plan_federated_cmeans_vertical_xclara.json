{
   "name": "Federated c-means algorithm for vertically data partitioning",
   "server_module": "fedxai_lib.algorithms.federated_cmeans_vertical.server",
   "client_module": "fedxai_lib.algorithms.federated_cmeans_vertical.client",
   "min_num_clients": 1,
   "parameters": {
      "num_clusters": 3,
      "centroid_seed": 0,
      "epsilon": 0.005,
      "lambda_factor": 2,
      "max_number_rounds": 100,
      "min_num_clients": 1,
      "dataset": "/datasets/xclara.csv"
   },
   "stages": [
       {
          "name": "Initialization of parties",
          "type": "run_once",
          "pipeline": [
             {
                "entity": "#server",
                "method": "init"
             },
             {
                "entity": "#available_clients",
                "method": "init"
             }
          ]
       },
       {
          "name": "Run federation",
          "type": "run_many",
          "stop_condition": "stop_federation",
          "pipeline": [
             {
                "entity": "#server",
                "method": "get_cluster_stats"
             },
             {
                "entity": "#available_clients",
                "method": "update_local_centers"
             },
             {
                "entity": "#server",
                "method": "process_round"
             }
          ]
       }
   ]
}